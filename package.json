{
	"name": "@subiz/campaign-template",
	"version": "0.1.61",
	"main": "./index.js",
	"scripts": {
		"serve": "vue-cli-service serve",
		"gen": "node ./generate.js",
		"build": "rm -rf dist && npx babel index.js -d dist && npx babel templates -d dist/templates && npx babel components -d dist/components && npx babel src -d dist/src",
